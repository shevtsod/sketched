{
  "name": "server",
  "private": true,
  "license": "UNLICENSED",
  "version": "0.0.0",
  "description": "A web game about drawing things from memory!",
  "homepage": "https://github.com/shevtsod/sketched#readme",
  "bugs": {
    "url": "https://github.com/shevtsod/sketched/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shevtsod/sketched.git"
  },
  "author": "Daniel Shevtsov",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0 --watch --preserveWatchOutput",
    "dev": "nest start --debug 0.0.0.0 --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "cli": "nest start --entryFile cli --",
    "cli:dev": "nest start --watchAll --entryFile cli --",
    "cli:prod": "node dist/cli",
    "prisma": "prisma"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "clearMocks": true
  },
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/express5": "^1.1.2",
    "@aws-sdk/client-s3": "^3.958.0",
    "@nestjs/apollo": "^13.2.3",
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/graphql": "^13.2.3",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "argon2": "^0.44.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "execa": "^9.6.1",
    "generate-password": "^1.7.1",
    "graphql": "^16.12.0",
    "nest-commander": "^3.20.1",
    "nestjs-pino": "^4.5.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.1.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.10",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@testcontainers/minio": "^11.11.0",
    "@testcontainers/postgresql": "^11.11.0",
    "@testcontainers/valkey": "^11.11.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "nest-commander-testing": "^3.5.1",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "testcontainers": "^11.10.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.16"
  }
}
