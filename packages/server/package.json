{
  "name": "server",
  "private": true,
  "license": "UNLICENSED",
  "version": "0.0.0",
  "description": "A web game about drawing things from memory!",
  "homepage": "https://github.com/shevtsod/sketched#readme",
  "bugs": {
    "url": "https://github.com/shevtsod/sketched/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shevtsod/sketched.git"
  },
  "author": "Daniel Shevtsov",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0 --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "cli": "nest start --entryFile cli --",
    "cli:dev": "nest start --watchAll --entryFile cli --",
    "cli:prod": "node dist/cli",
    "db": "drizzle-kit"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "clearMocks": true
  },
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/express5": "^1.1.2",
    "@aws-sdk/client-s3": "^3.946.0",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/platform-express": "^11.1.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.0",
    "drizzle-seed": "^0.3.1",
    "graphql": "^16.12.0",
    "nest-commander": "^3.20.1",
    "nestjs-pino": "^4.5.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@types/express": "^5.0.6",
    "@types/inquirer": "^9.0.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.15",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "nest-commander-testing": "^3.5.1",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.15"
  }
}
